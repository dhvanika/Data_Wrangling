q = ?$query=SELECT contractor_company_name ,contractor_trade ,contractor_trade_mapped ,contractor_license_number ,contractor_state_license ,contractor_full_name ,contractor_company_desc ,contractor_phone ,contractor_address_1 ,contractor_address_2 ,contractor_city ,contractor_state ,contractor_zip ,contractor_email LIMIT 15
q = ?$query=SELECT contractor_company_name ,contractor_trade ,contractor_trade_mapped ,contractor_license_number ,contractor_state_license ,contractor_full_name ,contractor_company_desc ,contractor_phone ,contractor_address_1 ,contractor_address_2 ,contractor_city ,contractor_state ,contractor_zip ,contractor_email , COUNT(*) AS count GROUP BY contractor_company_name ,contractor_phone ,contractor_email |> SELECT COUNT(contractor_company_name) as ccn_num , COUNT(contractor_phone) as cp_num , COUNT(contractor_email) as ce_num, SUM(count) as contractor_name_phone_email_totals LIMIT 15


